material IPA/Wall2
{
	technique
	{
    pass
    {
      scene_blend alpha_blend
      depth_write off

      ambient 0.725 0.865 0.605
      diffuse 0 1 0

      texture_unit
      {
        colour_op_ex source1 src_current src_current 1.0 1.0 1.0
        alpha_op_ex source1 src_manual src_current 0.5
      }
    }
	}
}

